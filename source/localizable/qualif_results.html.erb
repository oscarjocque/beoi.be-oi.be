---
title: Qualification Results
trianglify_colors: '"RdYlGn"'
---
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<section class="page-title page-title-4 image-bg">
  <%=page_header_empty_image_holder %>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h3 class="uppercase mb0"><%= t(:qualif_result_title) %> 2019</h3>
      </div>
    </div>
    <!--end of row-->
  </div>
  <!--end of container-->
</section>

<section class="altern">
  <div class="container">
    <div class="row ">
      <div class="col-sm-12 col-md-8 col-md-offset-2">
        <p class="lead mb64"><%= t(:qualif_intro_text) %></p>
      </div>
    </div><!--end of row-->
  </div><!--end of container-->
</section>

<section class="altern">
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 text-center">
        <h3 class="uppercase mb16 text-center"><%= t(:qualif_result_summary) %></h3>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <h4 class="uppercase mb16 text-center"><%= t(:qualif_result_stat_contestants) %></h4>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <p class="text-center"><%=
          t(:qualif_result_stat_contestants_text) % [config.beoi_year, data.results_2019_qualif.stats.total_contestants, data.results_2019_qualif.stats.school_total, data.results_2019_qualif.stats.school_org_total]
        %></p>
      </div>
      <div id="chart-cat" class="col-md-4" style="height: 200px;"></div>
      <div id="chart-lang" class="col-md-4" style="height: 200px;"></div>
      <div id="chart-gender" class="col-md-4" style="height: 200px;"></div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <h4 class="uppercase mb16 text-center"><%= t(:qualif_result_stat_results) %></h4>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <p class="text-center"><%= t(:qualif_result_stat_graph_expl) %></p>
        <p class="text-center"><%= t(:qualif_result_stat_thresholds) % [data.results_2019_qualif.stats.threshold_cadet, data.results_2019_qualif.stats.qualified_cadet, data.results_2019_qualif.stats.threshold_junior, data.results_2019_qualif.stats.qualified_junior, data.results_2019_qualif.stats.threshold_senior, data.results_2019_qualif.stats.qualified_senior, data.results_2019_qualif.stats.max_possible_score] %></p>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-12">
        <h5 class="uppercase mb16 text-center mt16" style=" margin-bottom: -10px;"><%= t(:Cadet) %></h5>
        <div id="chart-scores-cad" class="col-md-12" ></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-12">
        <h5 class="uppercase mb16 text-center mt16" style="margin-bottom: -10px;"><%= t(:Junior) %></h5>
        <div id="chart-scores-jun" class="col-md-12" ></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-12">
        <h5 class="uppercase mb16 text-center mt16" style="margin-bottom: -10px;"><%= t(:Senior) %></h5>
        <div id="chart-scores-sen" class="col-md-12" ></div>
      </div>
    </div>

  </div>
  <!--end of container-->
</section>

<section class="altern">
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 text-center">
        <h4 class="uppercase mb16 text-center"><%= t(:Cadet) %></h4>
        <h5 class="uppercase mb16 text-center"><%= t(:qualif_topx)%data.results_2019_qualif.contestants.cadet.size %> - <%= t(:qualif_on_total)%data.results_2019_qualif.stats.count_cadet %></h5>
      </div>
      <div class="col-sm-12 col-md-10 col-md-offset-1">
        <table class="table table-striped">
          <tbody>
            <tr><th>#</th><th><%= t(:column_name) %></th><th><%= t(:column_school) %></th><th><%= t(:column_score) %></th></tr>
            <% data.results_2019_qualif.contestants.cadet.each do |r| %>
            <tr>
              <td><%= r.position %></td>
              <td nowrap="nowrap"><%= r.name %></td>
              <td><%= r.school %> (<%= t(r.year) %>)</td>
              <td><strong><%= r.score %></strong></td>
            </tr>
            <% end %>
          </tbody>
        </table>

      </div>
    </div>
    <!--end of row-->

  </div>
  <!--end of container-->
</section>

<section class="altern">
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 text-center">
        <h4 class="uppercase mb16 text-center"><%= t(:Junior) %></h4>
        <h5 class="uppercase mb16 text-center"><%= t(:qualif_topx)%data.results_2019_qualif.contestants.junior.size %> - <%= t(:qualif_on_total)%data.results_2019_qualif.stats.count_junior %></h5>
      </div>
      <div class="col-sm-12 col-md-10 col-md-offset-1">
        <table class="table table-striped">
          <tbody>
            <tr><th>#</th><th><%= t(:column_name) %></th><th><%= t(:column_school) %></th><th><%= t(:column_score) %></th></tr>
            <% data.results_2019_qualif.contestants.junior.each do |r| %>
            <tr>
              <td><%= r.position %></td>
              <td nowrap="nowrap"><%= r.name %></td>
              <td><%= r.school %> (<%= t(r.year) %>)</td>
              <td><strong><%= r.score %></strong></td>
            </tr>
            <% end %>
          </tbody>
        </table>

      </div>
    </div>
    <!--end of row-->

  </div>
  <!--end of container-->
</section>

<section class="altern">
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 text-center">
        <h4 class="uppercase mb16 text-center"><%= t(:Senior) %></h4>
        <h5 class="uppercase mb16 text-center"><%= t(:qualif_topx)%data.results_2019_qualif.contestants.senior.size %> - <%= t(:qualif_on_total)%data.results_2019_qualif.stats.count_senior %></h5>
      </div>
      <div class="col-sm-12 col-md-10 col-md-offset-1">
        <table class="table table-striped">
          <tbody>
            <tr><th>#</th><th><%= t(:column_name) %></th><th><%= t(:column_school) %></th><th><%= t(:column_score) %></th></tr>
            <% data.results_2019_qualif.contestants.senior.each do |r| %>
            <tr>
              <td><%= r.position %></td>
              <td nowrap="nowrap"><%= r.name %></td>
              <td><%= r.school %> (<%= t(r.year) %>)</td>
              <td><strong><%= r.score %></strong></td>
            </tr>
            <% end %>
          </tbody>
        </table>

      </div>
    </div>
    <!--end of row-->

  </div>
  <!--end of container-->
</section>


<section class="altern">
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 text-center">
        <h4 class="uppercase mb16 text-center"><%= t(:qualif_top_schools) %></h4>
        <h5 class="mb16 text-center"><%= t(:qualif_top_schools_count_expl) %></h5>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-6 col-sm-offset-3 feature bordered" style="padding-bottom:8px;">
        <ol class="dl-horizontal category-list">
          <% data.results_2019_qualif.schools.nb_contestants.each do |s| %>
          <li><%= s.name %>: <%= s.value %> <%= t(:participants) %></li>
          <% end %>
        </ol>
      </div>
    </div>
  </div>
  <!--end of container-->
</section>

<script type="text/javascript">
// ["#e31d3b","#d53e4f","#f46d43","#fdae61","#fee08b","#ffffbf","#e6f598","#abdda4","#66c2a5","#3288bd","#5e4fa2"]'
google.charts.load('current', {'packages':['corechart','bar']});
google.charts.setOnLoadCallback(function() {
  var data = google.visualization.arrayToDataTable([
    ['<%= t(:Categories) %>', '<%= t(:qualif_chart_nbcontestants) %>'],
    ['<%= t(:Cadet) %>',     <%= data.results_2019_qualif.stats.count_cadet %>],
    ['<%= t(:Junior) %>',    <%= data.results_2019_qualif.stats.count_junior %>],
    ['<%= t(:Senior) %>',    <%= data.results_2019_qualif.stats.count_senior %>]
  ]);
  var options = {
    slices: {
      0: { color: '#e31d3b' },
      1: { color: '#f46d43' },
      2: { color: '#fdae61' }
    },
    backgroundColor: "transparent"
  };
  var chart = new google.visualization.PieChart(document.getElementById('chart-cat'));
  chart.draw(data, options);
});
google.charts.setOnLoadCallback(function() {
  var data = google.visualization.arrayToDataTable([
    ['<%= t(:qualif_chart_lang) %>', '<%= t(:qualif_chart_nbcontestants) %>'],
    ['<%= t(:qualif_chart_lang_fr) %>',    <%= data.results_2019_qualif.stats.count_fr %>],
    ['<%= t(:qualif_chart_lang_nl) %>',    <%= data.results_2019_qualif.stats.count_nl %>],
    ['<%= t(:qualif_chart_lang_de) %>',    <%= data.results_2019_qualif.stats.count_de %>]
  ]);
  var options = {
    slices: {
      0: { color: '#f46d43' },
      1: { color: '#fdae61' },
      2: { color: '#e31d3b' }
    },
    backgroundColor: "transparent"
  };
  var chart = new google.visualization.PieChart(document.getElementById('chart-lang'));
  chart.draw(data, options);
});
google.charts.setOnLoadCallback(function() {
  var data = google.visualization.arrayToDataTable([
    ['<%= t(:gender_field) %>', 'Nombre de participants'],
    ['<%= t(:gender_men) %>',     <%= data.results_2019_qualif.stats.count_male %>],
    ['<%= t(:gender_women) %>',      <%= data.results_2019_qualif.stats.count_female %>],
  ]);
  var options = {
    slices: {
      0: { color: '#fdae61' },
      1: { color: '#f46d43' },
    },
    backgroundColor: "transparent"
  };
  var chart = new google.visualization.PieChart(document.getElementById('chart-gender'));
  chart.draw(data, options);
});
google.charts.setOnLoadCallback(function() {
  var data = google.visualization.arrayToDataTable([
    ['', 'Score','Score'],
    <%= score_list_to_js_chart(data.results_2019_qualif.charts.cadet, data.results_2019_qualif.stats.threshold_cadet) %>
  ]);

  var options = {
    stacked: true,
    bars: 'vertical', // Required for Material Bar Charts.
    height: 150,
    series: { 0: {color: '#f46d43'}, 1: {color: '#66c2a5'} },
    legend: { position: "none" },
    bar: { groupWidth: "100%" },
    backgroundColor: "transparent",
    chartArea: { backgroundColor: "transparent" }
  };
  var chart = new google.charts.Bar(document.getElementById('chart-scores-cad'));
  chart.draw(data, google.charts.Bar.convertOptions(options));
});
google.charts.setOnLoadCallback(function() {
  var data = google.visualization.arrayToDataTable([
    ['', 'Score','Score'],
    <%= score_list_to_js_chart(data.results_2019_qualif.charts.junior, data.results_2019_qualif.stats.threshold_junior) %>
  ]);

  var options = {
    stacked: true,
    bars: 'vertical', // Required for Material Bar Charts.
    height: 150,
    series: { 0: {color: '#f46d43'}, 1: {color: '#66c2a5'} },
    legend: { position: "none" },
    bar: { groupWidth: "100%" },
    backgroundColor: "transparent",
    chartArea: { backgroundColor: "transparent" }
  };
  var chart = new google.charts.Bar(document.getElementById('chart-scores-jun'));
  chart.draw(data, google.charts.Bar.convertOptions(options));
});
google.charts.setOnLoadCallback(function() {
  var data = google.visualization.arrayToDataTable([
    ['', 'Score','Score'],
    <%= score_list_to_js_chart(data.results_2019_qualif.charts.senior, data.results_2019_qualif.stats.threshold_senior) %>
  ]);

  var options = {
    stacked: true,
    bars: 'vertical', // Required for Material Bar Charts.
    height: 150,
    series: { 0: {color: '#f46d43'}, 1: {color: '#66c2a5'} },
    legend: { position: "none" },
    bar: { groupWidth: "100%" },
    backgroundColor: "transparent",
    chartArea: { backgroundColor: "transparent" },
    vAxis: { maxValue: 40 }
  };

  var chart = new google.charts.Bar(document.getElementById('chart-scores-sen'));
  chart.draw(data, google.charts.Bar.convertOptions(options));
});
</script>


